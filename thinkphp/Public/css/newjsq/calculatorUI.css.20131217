.mtTB{margin-top: 56px;margin-bottom: 34px;}
.mtTB2{margin-top: 30px;margin-bottom: 15px;}
.mtB{margin-bottom: 15px;}

.calFBoxRPer{float:left;padding-left: 2px;}
.loanCmsC .calFBoxRPer a,.loanCmsC .calFBoxRPer a:visited,.loanCmsC .calFBoxRPer2 a,.loanCmsC .calFBoxRPer2 a:visited,.loanCmsC .calFBoxSel a,.loanCmsC .calFBoxSel a:visited{font-family:\5FAE\8F6F\96C5\9ED1,'Microsoft YaHei',SimHei,SimSun;font-size:16px;color:#292c30;text-shadow:0 1px 1px #a6adb0;display:block;height:36px;background-position:0 0;color:#2c2f30;text-align:center;line-height:35px;float:left;margin-right: 3px;}
.calFBoxRPer a,.calFBoxRPer a:visited{width:76px;background:url("../images/calculator/calFBoxRPer.png") no-repeat;}
.calFBoxRPer2 a,.calFBoxRPer2 a:visited{width:69px;background:url("../images/calculator/calFBoxRPer2.png") no-repeat;}
.calFBoxSel a,.calFBoxSel a:visited{width:102px;background:url("../images/calculator/calFBoxRPer3.png") no-repeat;}
.loanCmsC .calFBoxRPer a.curr,.loanCmsC .calFBoxRPer a:visited.curr,.loanCmsC .calFBoxRPer2 a.curr,.loanCmsC .calFBoxRPer2 a:visited.curr,.loanCmsC .calFBoxSel a.curr,.loanCmsC .calFBoxSel a:visited.curr{background-position:0 -35px;text-shadow:0 1px 1px #1e2021;color:#fff;}
a.calculatorBtn,a:visited.calculatorBtn{background:url("../images/calculator/calculatorBtn.gif") no-repeat;display:block;width:151px;height:35px;margin:33px auto;}

/*calculatorUI result*/
.calRInsert{width:100%;}
.loanCmsC a.calDetailT,.loanCmsC a:visited.calDetailT{background-color:#768185;border:1px solid #717171;color:#fff;font-size:14px;font-family:\5FAE\8F6F\96C5\9ED1,'Microsoft YaHei',SimHei,SimSun;display:block;text-align:center;height:38px;line-height:38px;width:642px;margin:0 auto;position:relative;
	-webkit-border-radius:5px;
	-moz-border-radius:5px;
	border-radius:5px;}
.loanCmsC a:hover.calDetailT{background-color:#707b7f;text-shadow:0 1px 1px #515a5d;}
.loanCmsC a.calDetailT i,.loanCmsC a:visited.calDetailT i{display:block;width:22px;height:22px;position:absolute;right:20px;top:7px;background:url("../images/calculator/calDetailIcon.png") no-repeat 0 0;}
.loanCmsC a.calDetailT.curr i,.loanCmsC a:visited.calDetailT.curr i{background:url("../images/calculator/calDetailIcon.png") no-repeat 0 -22px;}

.calResult,.calDetailTable{border:1px solid #bfbfbf;width:644px;margin:12px auto;_border-collapse:collapse;
	-webkit-box-shadow:0 3px 3px #eee;
	-moz-box-shadow:0 3px 3px #eee;
	box-shadow:0 3px 3px #eee;
	-webkit-border-radius:5px;
	-moz-border-radius:5px;
	border-radius:5px;
}
.loanCmsC table.calResult tr th,.loanCmsC table.calResult tr td,.loanCmsC table.calDetailTable tr th,.loanCmsC table.calDetailTable tr td{height:40px;border-bottom:1px solid #bfbfbf;}
.loanCmsC table.calResult tr td strong,.loanCmsC table.calResult tr td strong,.loanCmsC table.calDetailTable tbody tr td strong{color:#C10000;}
.loanCmsC table.calResult tbody tr th{width:320px;}
.loanCmsC table.calResult tbody tr th,.loanCmsC table.calResult tbody tr td{text-align:left;text-indent:20px;}
.loanCmsC table.calDetailTable tr th,.loanCmsC table.calDetailTable tr td{text-align:center;}
.loanCmsC table.calResult thead tr th,.loanCmsC table.calDetailTable thead tr th{background-color:#eaeaea;color:#f16e04;}
.loanCmsC table.calResult tbody tr th,.loanCmsC table.calResult tbody tr td,.loanCmsC table.calDetailTable tbody tr th,.loanCmsC table.calDetailTable tbody tr td{color:#515151;border-right:1px solid #bfbfbf;}
.loanCmsC table.calResult tbody tr th:last-child,.loanCmsC table.calResult tbody tr td:last-child,.loanCmsC table.calDetailTable tbody tr th:last-child,.loanCmsC table.calDetailTable tbody tr td:last-child{border-right:0;}
.loanCmsC table.calResult tbody tr:last-child td,.loanCmsC table.calResult tbody tr:last-child th,.loanCmsC table.calDetailTable tbody tr:last-child td{border-bottom:0;}
.calFBoxL2.nl,.calFBoxRPer.nl{float:none;}
.calFBoxL2.mb{margin-bottom: 9px;}
a.calculatorBtn.mt,a:visited.calculatorBtn.mt{margin-top:20px;}
.loanCmsC table.calResult td,.loanCmsC table.calDetailTable td{border-top:0;border-right:1px solid #bfbfbf;border-left:0;}
.loanCmsC table.calResult,.loanCmsC table.calDetailTable{border-right:1px solid #bfbfbf;border-bottom:1px solid #bfbfbf;}

/*calFBoxSel*/
.calFBox{position:relative;}
.calFBoxSel{width:102px;background-color:#fff;position:absolute;right:30px;
	-webkit-border-radius:5px;
	-moz-border-radius:5px;
	border-radius:5px;}
.loanCmsC .calFBoxSel a,.loanCmsC .calFBoxSel a:visited{position:relative;text-align:left;text-indent:10px;_margin-right:0;}
.calFBoxSel a i,.calFBoxSel a:visited i{background:url("../images/calculator/iconSel.png") no-repeat 0 0;display:block;width:12px;height:9px;position:absolute;right:8px;top:14px;_overflow:hidden;}
.calFBoxSel a i.on,.calFBoxSel a:visited i.on{background:url("../images/calculator/iconSel.png") no-repeat 0 -9px;}
.calFBoxSelChild{width:102px;height:142px;position:relative; margin:0px auto; padding:0px;border:1px solid #ccc;border-top:0;overflow: hidden;}
.calFBoxSelChild ul li{text-align:center;line-height:20px;display:block;color:#5f5f5f;cursor:pointer;} 
.calFBoxSelChild ul li.curr{background-color:#63686a;color:#fff;}
.calFBoxSelChild.none{display:none;}
.calFBoxSel a.bgOn,.calFBoxSel a:visited.bgOn{background:url("../images/calculator/calFBoxRPer3.png") no-repeat 0 -36px;color:#fff;text-shadow:0 1px 1px #484d4e;}

