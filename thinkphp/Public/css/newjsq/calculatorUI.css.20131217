.calFBoxL1{margin-top: 20px;}
.calFBoxL2{margin-top: 8px;}

.calNumHide{width:300px;height:46px;line-height:46px;text-align:right;background-color:#ffcb31;color:#907310;font-size:30px;border:1px solid #fff;font-family:Arial, Helvetica, sans-serif;padding-right: 10px;
	-webkit-border-radius:5px;
